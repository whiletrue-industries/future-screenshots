{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "app": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "less",
          "skipTests": true
        },
        "@schematics/angular:service": {
          "skipTests": true
        }
      },
      "root": "projects/app",
      "sourceRoot": "projects/app/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/app",
            "index": "projects/app/src/index.html",
            "browser": "projects/app/src/main.ts",
            "polyfills": [
              "zone.js",
              "@angular/localize/init"
            ],
            "tsConfig": "projects/app/tsconfig.app.json",
            "inlineStyleLanguage": "less",
            "assets": [
              {
                "glob": "**/*",
                "input": "projects/app/public"
              }
            ],
            "styles": [
              "projects/app/src/styles.less"
            ],
            "scripts": [],
            "localize": ["he", "en", "nl"]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "1500kB",
                  "maximumError": "2MB"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "16kB",
                  "maximumError": "50kB"
                }
              ],
              "outputHashing": "all",
              "optimization": {
                  "scripts": true,
                  "fonts": false,
                  "styles": false
              }
            },
            "he": {
              "localize": ["he"]
            },
            "nl": {
              "localize": ["nl"]
            },
            "development": {
              "optimization": {
                  "scripts": true,
                  "fonts": false,
                  "styles": false
              },
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "app:build:production"
            },
            "development": {
              "buildTarget": "app:build:development"
            },
            "he": {
              "buildTarget": "app:build:he"
            },
            "nl": {
              "buildTarget": "app:build:nl"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing",
              "@angular/localize/init"
            ],
            "tsConfig": "projects/app/tsconfig.spec.json",
            "inlineStyleLanguage": "less",
            "assets": [
              {
                "glob": "**/*",
                "input": "projects/app/public"
              }
            ],
            "styles": [
              "projects/app/src/styles.less"
            ],
            "scripts": []
          }
        }
      },
      "i18n": {
        "sourceLocale": "en-US",
        "locales": {
          "he": {
            "translation": "projects/app/src/locale/messages.he.xlf"
          },
          "en": {
            "translation": "projects/app/src/locale/messages.en.xlf",
            "baseHref": "/"
          },
          "nl": {
            "translation": "projects/app/src/locale/messages.nl.xlf"
          }
        }
      }
    }
  },
  "cli": {
    "analytics": false
  }
}
